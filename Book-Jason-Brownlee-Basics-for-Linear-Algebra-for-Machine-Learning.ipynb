   "source": [
    "# create array\n",
    "from numpy import array\n",
    "\n",
    "# create array\n",
    "l = [1.0, 2.0, 3.0]\n",
    "a = array(l)\n",
    "\n",
    "# display array\n",
    "print(a)\n",
    "\n",
    "# display array shape\n",
    "print(a.shape)\n",
    "\n",
    "# display array data type\n",
    "print(a.dtype)"
   ]
